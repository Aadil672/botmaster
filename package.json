{
  "name": "botmaster",
  "version": "2.0.0",
  "description": "Framework allowing developers to write bots that are agnostic with respect to the channel used by their users (messenger, telegram etc...)",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nodemon --legacy-watch ./example_jd.js",
    "test": "istanbul cover _mocha -- --recursive tests",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "directories": {
    "tests": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdwuarin/botmaster"
  },
  "bugs": {
    "url": "https://github.com/jdwuarin/botmaster/issues"
  },
  "keywords": [
    "bot",
    "framework",
    "toolkit",
    "botmaster",
    "slack",
    "messenger",
    "telegram",
    "twitter",
    "bot library"
  ],
  "dependencies": {
    "body-parser": "^1.15.0",
    "crypto": "0.0.3",
    "express": ">=4x  .4.x <5.0.0 || >=5.0.0-alpha.2 < 7.0.0",
    "jfs": "^0.2.6",
    "lodash": "^4.16.1",
    "request-promise": "^4.1.1",
    "socket.io": "^1.5.0",
    "twit": "^2.2.5",
    "ware": "^1.3.0"
  },
  "engines": {
    "node": ">=4.x.x <7.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "mocha-eslint": "^2.1.1",
    "nodemon": "^1.10.2"
  },
  "author": "JD Wuarin <jwuarin@uk.ibm.com>",
  "license": "MIT"
}
